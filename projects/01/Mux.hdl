/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
        Not(in=sel, out=t);
        Nand(a=a, b=t, out=x);
        Nand(a=sel, b=b, out=y);
        Nand(a=x, b=y, out=out);
}
